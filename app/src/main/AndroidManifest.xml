<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homubee.waterrate">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_main"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WaterRate">
        <activity
            android:name=".view.AppInfoActivity"
            android:parentActivityName=".view.MainActivity"></activity>
        <activity android:name=".view.ResultActivity" />
        <activity android:name=".view.InitializePrivateActivity" />
        <activity android:name=".view.CalculateActivity" />
        <activity android:name=".view.InitializePublicActivity" />
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>